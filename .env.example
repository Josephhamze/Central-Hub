# Docker Compose Environment Variables for AWS EC2 Deployment
# Copy this file to .env and update values for your environment

# PostgreSQL Configuration
POSTGRES_USER=alphapms_user
POSTGRES_PASSWORD=alphapms_password
POSTGRES_DB=alphapms_database

# Redis Configuration
REDIS_PASSWORD=alphapms_redis_password

# JWT Configuration (CHANGE IN PRODUCTION!)
JWT_SECRET=your-super-secret-jwt-key-change-in-production
JWT_REFRESH_SECRET=your-super-secret-refresh-key-change-in-production
JWT_EXPIRATION=15m
JWT_REFRESH_EXPIRATION=7d

# Frontend API URL
VITE_API_URL=https://api.alphapms.app/api/v1
